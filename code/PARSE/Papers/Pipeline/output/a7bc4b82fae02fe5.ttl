askg-data:Paper-a7bc4b82fae02fe5-Section-12 a askg-onto:Section ;
    rdfs:label "Section 12"@en ;
    domo:Text "Features"@en ;
    askg-onto:hasParagraph askg-data:Paper-a7bc4b82fae02fe5-Section-12-Paragraph-121 ;
