askg-data:Paper-d6e3ff97abc852d1-Section-2 a askg-onto:Section ;
    rdfs:label "Section 2"@en ;
    domo:Text "Abstract"@en ;
    askg-onto:hasParagraph askg-data:Paper-d6e3ff97abc852d1-Section-2-Paragraph-21 ;
