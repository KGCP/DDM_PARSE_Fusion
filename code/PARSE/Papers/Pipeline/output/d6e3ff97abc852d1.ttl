askg-data:Paper-d6e3ff97abc852d1-Section-10 a askg-onto:Section ;
    rdfs:label "Section 10"@en ;
    domo:Text "3.2 On The Importance Of Moments"@en ;
    askg-onto:hasParagraph askg-data:Paper-d6e3ff97abc852d1-Section-10-Paragraph-101 ;
