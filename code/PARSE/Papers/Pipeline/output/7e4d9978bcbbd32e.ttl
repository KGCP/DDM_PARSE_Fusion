askg-data:Paper-7e4d9978bcbbd32e-Section-1 a askg-onto:Section ;
    rdfs:label "Section 1"@en ;
    domo:Text "Deceptive Games"@en ;
    askg-onto:hasParagraph askg-data:Paper-7e4d9978bcbbd32e-Section-1-Paragraph-11 ;
