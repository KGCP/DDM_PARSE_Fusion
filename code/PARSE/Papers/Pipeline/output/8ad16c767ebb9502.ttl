askg-data:Paper-8ad16c767ebb9502-Section-2-Paragraph-27-Sentence-279 a askg-onto:Sentence ;
    rdfs:label "Sentence 9"@en ;
    domo:Text "However, below 200 meters, near the ground, thermals' irregular shape makes the lack of exploration a notable drawback, as we show in this paper."@en ;
    askg-onto:inSentence "However, below 200 meters, near the ground, thermals' irregular shape makes the lack of exploration a notable drawback, as we show in this paper."^^xsd:string ;
    askg-onto:index "9"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-irregular_shape,
        askg-data:Entity-lack_of_exploration,
        askg-data:Entity-thermals,
