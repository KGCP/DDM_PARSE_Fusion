    rdfs:label "Sentence 10"@en ;
    domo:Text "To minimize their effect, after each flight that counted towards this experiment's results we switched the assignment of thermalling controllers to airframes."@en ;
    askg-onto:inSentence "To minimize their effect, after each flight that counted towards this experiment's results we switched the assignment of thermalling controllers to airframes."^^xsd:string ;
    askg-onto:index "10"^^xsd:int ;
