askg-data:Paper-8ad16c767ebb9502-Section-11-Paragraph-1117-Sentence-111711 a askg-onto:Sentence ;
    rdfs:label "Sentence 11"@en ;
    domo:Text "At each test site, POMDSoar and ArduSoar completed an equal number of experimental flights on each sUAV."@en ;
    askg-onto:inSentence "At each test site, POMDSoar and ArduSoar completed an equal number of experimental flights on each sUAV."^^xsd:string ;
    askg-onto:index "11"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-ardusoar,
        askg-data:Entity-experimental_flights,
