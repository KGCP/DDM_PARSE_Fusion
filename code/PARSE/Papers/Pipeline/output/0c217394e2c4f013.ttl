askg-data:Paper-0c217394e2c4f013-Section-1 a askg-onto:Section ;
    rdfs:label "Section 1"@en ;
    domo:Text "A Van Benthem Theorem For Antitative Probabilistic Modal Logic"@en ;
    askg-onto:hasParagraph askg-data:Paper-0c217394e2c4f013-Section-1-Paragraph-11,
        askg-data:Paper-0c217394e2c4f013-Section-1-Paragraph-12 ;
