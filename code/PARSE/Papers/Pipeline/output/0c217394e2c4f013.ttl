        askg-data:Paper-0c217394e2c4f013-Section-8-Paragraph-81-Sentence-812,
        askg-data:Paper-0c217394e2c4f013-Section-8-Paragraph-81-Sentence-813 ;
    askg-onto:index "1"^^xsd:int .

