    rdfs:label "Section 1"@en ;
    domo:Text "On Improving The Balance Between The Completion Time And Decoding Delay In Instantly Decodable Network Coded Systems"@en ;
    askg-onto:hasParagraph askg-data:Paper-0fcdabfd4a3b1e47-Section-1-Paragraph-11,
        askg-data:Paper-0fcdabfd4a3b1e47-Section-1-Paragraph-12 ;
