        askg-data:Paper-8549220a250c3e09-Section-8-Paragraph-82-Sentence-824,
        askg-data:Paper-8549220a250c3e09-Section-8-Paragraph-82-Sentence-825 ;
    askg-onto:index "2"^^xsd:int .

