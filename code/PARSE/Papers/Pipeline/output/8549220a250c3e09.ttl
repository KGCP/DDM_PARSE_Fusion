    rdfs:label "Sentence 1"@en ;
    domo:Text "Cooperative protocols can be divided into several families depending on the processing performed at relaying nodes."@en ;
    askg-onto:inSentence "Cooperative protocols can be divided into several families depending on the processing performed at relaying nodes."^^xsd:string ;
    askg-onto:index "1"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-cooperative_protocols,
