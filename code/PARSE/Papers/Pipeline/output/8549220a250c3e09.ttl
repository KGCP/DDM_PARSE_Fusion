askg-data:Paper-8549220a250c3e09-Section-4-Paragraph-42-Sentence-422 a askg-onto:Sentence ;
    rdfs:label "Sentence 2"@en ;
    domo:Text "One of these families is the group of *decode-and-forward* (DF) protocols."@en ;
    askg-onto:inSentence "One of these families is the group of *decode-and-forward* (DF) protocols."^^xsd:string ;
    askg-onto:index "2"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-decode-and-forward,
        askg-data:Entity-protocols .

askg-data:Paper-8549220a250c3e09-Section-4-Paragraph-42-Sentence-423 a askg-onto:Sentence ;
    rdfs:label "Sentence 3"@en ;
    domo:Text "As the name suggests, relays have to decode and re-encode the received signals before forwarding them."@en ;
    askg-onto:inSentence "As the name suggests, relays have to decode and re-encode the received signals before forwarding them."^^xsd:string ;
    askg-onto:index "3"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-received_signals,
        askg-data:Entity-relays .

askg-data:Paper-8549220a250c3e09-Section-4-Paragraph-42-Sentence-424 a askg-onto:Sentence ;
    rdfs:label "Sentence 4"@en ;
    domo:Text "Decoding at the relays has a higher complexity than linear processing such as amplify-and-forward (AF) strategy."@en ;
    askg-onto:inSentence "Decoding at the relays has a higher complexity than linear processing such as amplify-and-forward (AF) strategy."^^xsd:string ;
    askg-onto:index "4"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-amplify-and-forward_af_strategy,
