        askg-data:Paper-947cd533a86e99f8-Section-11-Paragraph-115-Sentence-1155,
        askg-data:Paper-947cd533a86e99f8-Section-11-Paragraph-115-Sentence-1156,
        askg-data:Paper-947cd533a86e99f8-Section-11-Paragraph-115-Sentence-1157 ;
    askg-onto:index "5"^^xsd:int .

