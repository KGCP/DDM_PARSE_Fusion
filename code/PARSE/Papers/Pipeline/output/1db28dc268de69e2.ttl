askg-data:Paper-1db28dc268de69e2-Section-23-Paragraph-232-Sentence-2321 a askg-onto:Sentence ;
    rdfs:label "Sentence 1"@en ;
    domo:Text "$${\\cal N}\\!\\left(\\frac{(1-\\rho)w_{0}(1-\\alpha\\lambda)^{n+1}[1-(\\frac{\\rho}{1-\\alpha\\lambda})^{n-1}]}{1-\\alpha\\lambda-\\rho},\\frac{1-\\rho}{1+\\rho}\\frac{\\alpha\\sigma^{2}\\kappa}{\\lambda}\\right)\\tag{65}$$"@en ;
    askg-onto:inSentence "$${\\cal N}\\!\\left(\\frac{(1-\\rho)w_{0}(1-\\alpha\\lambda)^{n+1}[1-(\\frac{\\rho}{1-\\alpha\\lambda})^{n-1}]}{1-\\alpha\\lambda-\\rho},\\frac{1-\\rho}{1+\\rho}\\frac{\\alpha\\sigma^{2}\\kappa}{\\lambda}\\right)\\tag{65}$$"^^xsd:string ;
