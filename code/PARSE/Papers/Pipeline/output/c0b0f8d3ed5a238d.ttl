askg-data:Paper-c0b0f8d3ed5a238d-Section-2-Paragraph-21-Sentence-216 a askg-onto:Sentence ;
    rdfs:label "Sentence 6"@en ;
    domo:Text "Since, 3D point clouds do not follow a Euclidean topology, standard 2/3D Convolutional Neural Networks offer limited representation capability."@en ;
    askg-onto:inSentence "Since, 3D point clouds do not follow a Euclidean topology, standard 2/3D Convolutional Neural Networks offer limited representation capability."^^xsd:string ;
    askg-onto:index "6"^^xsd:int ;
