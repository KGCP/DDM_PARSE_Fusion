    rdfs:label "Sentence 4"@en ;
    domo:Text "This fully differentiable layer performs two critical tasks in succession."@en ;
    askg-onto:inSentence "This fully differentiable layer performs two critical tasks in succession."^^xsd:string ;
    askg-onto:index "4"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-this_fully_differentiable_layer,
