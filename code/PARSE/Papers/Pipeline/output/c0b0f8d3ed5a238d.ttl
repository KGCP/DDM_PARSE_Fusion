askg-data:Paper-c0b0f8d3ed5a238d-Section-2-Paragraph-21-Sentence-217 a askg-onto:Sentence ;
    rdfs:label "Sentence 7"@en ;
    domo:Text "Therefore, in the **second** step, it uses a novel 3D convolution operator functioning inside the unit ball (B 3) to extract useful volumetric features."@en ;
    askg-onto:inSentence "Therefore, in the **second** step, it uses a novel 3D convolution operator functioning inside the unit ball (B 3) to extract useful volumetric features."^^xsd:string ;
    askg-onto:index "7"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-3d_convolution_operator,
        askg-data:Entity-unit_ball_b_3 .

