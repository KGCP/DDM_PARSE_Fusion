    domo:Text "The data generated by the Internet of Things are valuable and have the potential to drive innovative and novel applications."@en ;
    askg-onto:inSentence "The data generated by the Internet of Things are valuable and have the potential to drive innovative and novel applications."^^xsd:string ;
    askg-onto:index "2"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-data,
