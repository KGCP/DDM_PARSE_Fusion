        askg-data:Paper-48bad0352f6f58e0-Section-7-Paragraph-72-Sentence-722,
        askg-data:Paper-48bad0352f6f58e0-Section-7-Paragraph-72-Sentence-723 ;
    askg-onto:index "2"^^xsd:int .

