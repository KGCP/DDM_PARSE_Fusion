askg-data:Paper-6e7ff6a1ada6bee0-Section-20 a askg-onto:Section ;
    rdfs:label "Section 20"@en ;
    domo:Text "B. Backcom For Biomedical Applications"@en ;
    askg-onto:hasParagraph askg-data:Paper-6e7ff6a1ada6bee0-Section-20-Paragraph-201 ;
    askg-onto:index "20"^^xsd:int ;
