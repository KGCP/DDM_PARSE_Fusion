    domo:Text "V. Iot Applications For Backcom A. Backcom For Smart Homes/Cities"@en ;
    askg-onto:hasParagraph askg-data:Paper-6e7ff6a1ada6bee0-Section-19-Paragraph-191,
        askg-data:Paper-6e7ff6a1ada6bee0-Section-19-Paragraph-192,
