askg-data:Paper-aefec0b8ec106bef-Section-4 a askg-onto:Section ;
    rdfs:label "Section 4"@en ;
    domo:Text "2.1 Smart Environment"@en ;
    askg-onto:hasParagraph askg-data:Paper-aefec0b8ec106bef-Section-4-Paragraph-41 ;
