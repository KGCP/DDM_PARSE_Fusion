        askg-data:Paper-bcdbe3b19581df2e-Section-24-Paragraph-241-Sentence-2414,
        askg-data:Paper-bcdbe3b19581df2e-Section-24-Paragraph-241-Sentence-2415 ;
    askg-onto:index "1"^^xsd:int .

