    rdfs:label "Sentence 4"@en ;
    domo:Text "A variety of approaches have been proposed in the literature to remove or reduce this interference [13]–[18]."@en ;
    askg-onto:inSentence "A variety of approaches have been proposed in the literature to remove or reduce this interference [13]–[18]."^^xsd:string ;
    askg-onto:index "4"^^xsd:int ;
