        askg-data:Paper-ac247c6c2ffb44ab-Section-2-Paragraph-22-Sentence-222,
        askg-data:Paper-ac247c6c2ffb44ab-Section-2-Paragraph-22-Sentence-223 ;
    askg-onto:index "2"^^xsd:int .

