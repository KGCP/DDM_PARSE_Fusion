askg-data:Paper-69c1c08947fe26c0-Section-15-Paragraph-151-Sentence-1513 a askg-onto:Sentence ;
    rdfs:label "Sentence 3"@en ;
    domo:Text "We propose alternative semantics for hybrid planning, but the purpose of doing so is not to supersede pddl+, but rather, to offer a complementary and useful view on the problem, compatible with existing tools to a great degree."@en ;
    askg-onto:inSentence "We propose alternative semantics for hybrid planning, but the purpose of doing so is not to supersede pddl+, but rather, to offer a complementary and useful view on the problem, compatible with existing tools to a great degree."^^xsd:string ;
    askg-onto:index "3"^^xsd:int ;
