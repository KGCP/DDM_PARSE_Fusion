askg-data:Paper-fff5d1d72c7db767-Section-4 a askg-onto:Section ;
    rdfs:label "Section 4"@en ;
    domo:Text "2. Related Work"@en ;
    askg-onto:hasParagraph askg-data:Paper-fff5d1d72c7db767-Section-4-Paragraph-41 ;
