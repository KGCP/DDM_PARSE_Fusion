askg-data:Paper-82422cb92b97128d-Section-9 a askg-onto:Section ;
    rdfs:label "Section 9"@en ;
    domo:Text "4 Related Work"@en ;
    askg-onto:hasParagraph askg-data:Paper-82422cb92b97128d-Section-9-Paragraph-91 ;
