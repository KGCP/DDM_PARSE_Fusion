askg-data:Paper-bf21964d46ba1c62-Section-14 a askg-onto:Section ;
    rdfs:label "Section 14"@en ;
    domo:Text "4.2.2 Big Data Processing Model"@en ;
    askg-onto:hasParagraph askg-data:Paper-bf21964d46ba1c62-Section-14-Paragraph-141 ;
