    rdfs:label "Section 1"@en ;
    domo:Text "Transmission And Scheduling Aspects Of Distributed Storage And Their Connections With Index Coding"@en ;
    askg-onto:hasParagraph askg-data:Paper-bd33c8df9580fe89-Section-1-Paragraph-11 ;
