        askg-data:Paper-361297ffb5697fe9-Section-4-Paragraph-41-Sentence-415,
        askg-data:Paper-361297ffb5697fe9-Section-4-Paragraph-41-Sentence-416,
        askg-data:Paper-361297ffb5697fe9-Section-4-Paragraph-41-Sentence-417,
        askg-data:Paper-361297ffb5697fe9-Section-4-Paragraph-41-Sentence-418,
        askg-data:Paper-361297ffb5697fe9-Section-4-Paragraph-41-Sentence-419 ;
    askg-onto:index "1"^^xsd:int .

