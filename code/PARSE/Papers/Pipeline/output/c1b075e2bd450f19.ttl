askg-data:Paper-c1b075e2bd450f19-Section-2 a askg-onto:Section ;
    rdfs:label "Section 2"@en ;
    domo:Text "Abstract"@en ;
    askg-onto:hasParagraph askg-data:Paper-c1b075e2bd450f19-Section-2-Paragraph-21 ;
