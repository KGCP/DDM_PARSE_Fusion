    rdfs:label "Sentence 7"@en ;
    domo:Text "The low Mach solver uses Crank-Nicolson time advancement and an iterative predictor corrector scheme."@en ;
    askg-onto:inSentence "The low Mach solver uses Crank-Nicolson time advancement and an iterative predictor corrector scheme."^^xsd:string ;
    askg-onto:index "7"^^xsd:int ;
