askg-data:Paper-d0c22801ce0f0c4d-Section-4 a askg-onto:Section ;
    rdfs:label "Section 4"@en ;
    domo:Text "1 Introduction And Background"@en ;
    askg-onto:hasParagraph askg-data:Paper-d0c22801ce0f0c4d-Section-4-Paragraph-41 ;
