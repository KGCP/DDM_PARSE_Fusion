    rdfs:label "Sentence 8"@en ;
    domo:Text "We then use the largest such merged binary blob and crop the sequence to a box containing this blob."@en ;
    askg-onto:inSentence "We then use the largest such merged binary blob and crop the sequence to a box containing this blob."^^xsd:string ;
    askg-onto:index "8"^^xsd:int ;
