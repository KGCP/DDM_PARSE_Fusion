askg-data:Paper-c0a2dfc396d72ee5-Section-18-Paragraph-182-Sentence-1822 a askg-onto:Sentence ;
    rdfs:label "Sentence 2"@en ;
    domo:Text "The preprocessing pipeline is illustrated in Figure 4."@en ;
    askg-onto:inSentence "The preprocessing pipeline is illustrated in Figure 4."^^xsd:string ;
    askg-onto:index "2"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-figure_4,
