        askg-data:Entity-parts_of_human_body .

askg-data:Paper-c0a2dfc396d72ee5-Section-18-Paragraph-182-Sentence-1824 a askg-onto:Sentence ;
    rdfs:label "Sentence 4"@en ;
    domo:Text "We then dilate these blobs using a 11 × 11 dilation filter to capture details surrounding them."@en ;
    askg-onto:inSentence "We then dilate these blobs using a 11 × 11 dilation filter to capture details surrounding them."^^xsd:string ;
    askg-onto:index "4"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-blobs,
        askg-data:Entity-dilation_filter .

askg-data:Paper-c0a2dfc396d72ee5-Section-18-Paragraph-182-Sentence-1825 a askg-onto:Sentence ;
    rdfs:label "Sentence 5"@en ;
    domo:Text "This is followed by Gaussian smoothing and connected components analysis to find blobs connected to each other."@en ;
    askg-onto:inSentence "This is followed by Gaussian smoothing and connected components analysis to find blobs connected to each other."^^xsd:string ;
    askg-onto:index "5"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-blobs,
        askg-data:Entity-connected_components_analysis,
        askg-data:Entity-gaussian_smoothing,
        askg-data:Entity-technique .

