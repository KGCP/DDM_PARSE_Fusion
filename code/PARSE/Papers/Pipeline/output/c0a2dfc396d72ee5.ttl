askg-data:Paper-c0a2dfc396d72ee5-Section-18-Paragraph-182-Sentence-18210 a askg-onto:Sentence ;
    rdfs:label "Sentence 10"@en ;
    domo:Text "We use these resized frames for computing optical flow using the TVL1 OpenCV implementation."@en ;
    askg-onto:inSentence "We use these resized frames for computing optical flow using the TVL1 OpenCV implementation."^^xsd:string ;
    askg-onto:index "10"^^xsd:int ;
    askg-onto:mentions askg-data:Entity-optical_flow,
        askg-data:Entity-tvl1_opencv .

askg-data:Paper-c0a2dfc396d72ee5-Section-18-Paragraph-182-Sentence-18211 a askg-onto:Sentence ;
    rdfs:label "Sentence 11"@en ;
    domo:Text "Each flow image is then thresholded to ± 20 pixels, rescaled to 0–255, and saved as a JPEG image for storage efficiency as described in [67]."@en ;
    askg-onto:inSentence "Each flow image is then thresholded to ± 20 pixels, rescaled to 0–255, and saved as a JPEG image for storage efficiency as described in [67]."^^xsd:string ;
