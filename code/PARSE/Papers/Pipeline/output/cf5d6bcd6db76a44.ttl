    rdfs:label "Sentence 3"@en ;
    domo:Text "For example, let us analyze a well-known APDD-reduction technique called instantly decodable network coding (IDNC)."@en ;
    askg-onto:inSentence "For example, let us analyze a well-known APDD-reduction technique called instantly decodable network coding (IDNC)."^^xsd:string ;
    askg-onto:index "3"^^xsd:int ;
