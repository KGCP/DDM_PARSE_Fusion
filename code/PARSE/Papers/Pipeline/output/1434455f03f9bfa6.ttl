askg-data:Paper-1434455f03f9bfa6-Section-20-Paragraph-2011-Sentence-20111 a askg-onto:Sentence ;
    rdfs:label "Sentence 1"@en ;
    domo:Text "As expected, the heuristic LoopElim and Greedy algorithms have the fastest runtime, being on the order of milliseconds per query even for long trajectories (Figure 4)."@en ;
    askg-onto:inSentence "As expected, the heuristic LoopElim and Greedy algorithms have the fastest runtime, being on the order of milliseconds per query even for long trajectories (Figure 4)."^^xsd:string ;
