[{"name": "jamie hicks", "affiliation": "university of oxford"}, {"name": "andreas heilmann", "affiliation": "university of oxford"}, {"name": "petra vasko", "affiliation": "university of oxford"}, {"name": "petra vasko", "affiliation": "university of jyvaskyla"}, {"name": "jose m goicoechea", "affiliation": "university of oxford"}, {"name": "simon aldridge", "affiliation": "university of oxford"}], "id": 67393510}, {"title": "a mixed valence tri zinc complex lznznznl l bulky amide bearing a linear chain of two coordinate zinc atoms", "year": 2015, "source": "angewandte chemie", "authors": [{"name": "jamie hicks", "affiliation": "monash university"}, {"name": "emma j underhill", "affiliation": "monash university"}, {"name": "christos e kefalidis", "affiliation": "university of toulouse"}, {"name": "laurent maron", "affiliation": "university of toulouse"}, {"name": "cameron jones", "affiliation": "monash university"}], "id": 67393510}], "ANDS-Grants": [{"description": "This project aims to develop a range of bio-inspired, mixed metal iron-molybdenum complexes that are capable of activating molecular nitrogen, N2, at ambient pressure and temperature. The activation of atmospheric N2 is performed on a multi-million tonne scale each year and is key to a number of industrial processes. The project expects to generate new knowledge in the area of organometallic chemistry, specifically with regards to molecular metal-metal bonding and subsequent reactivity towards the activation of N2. Expected outcomes include new and improved catalysts, which will provide significant financial benefits to industry, as well as benefiting the environment by reducing energy demand.", "title": "Discovery Early Career Researcher Award - Grant ID: DE190100524", "id": "1378029", "key": "http://purl.org/au-research/grants/arc/DE190100524", "researchers": ["Dr Jamie Hicks"], "subject_type": null, "subject_value_resolved": null, "institutions": null, "status": "active", "fundingAmount": 422574, "f