ch"}, {"name": "sanjay kumar bhadada"}, {"name": "ashok sharma"}, {"name": "v suresh"}, {"name": "amod gupta"}, {"name": "preet mohinder singh"}, {"name": "a chakarbarti"}, {"name": "r j dash"}], "id": 43893605}, {"title": "syntheses of 2 methoxyestradiol and eugenol template based diarylpropenes as non steroidal anticancer agents", "year": 2014, "source": "rsc advances", "authors": [{"name": "vinay pathak", "affiliation": "central institute of medicinal and aromatic plants"}, {"name": "imran ahmad", "affiliation": "central institute of medicinal and aromatic plants"}, {"name": "amandeep kaur kahlon", "affiliation": "central institute of medicinal and aromatic plants"}, {"name": "mohammad hasanain", "affiliation": "central drug research institute"}, {"name": "sandeep k sharma", "affiliation": "central drug research institute"}, {"name": "kishore k srivastava", "affiliation": "central drug research institute"}, {"name": "jayanta sarkar", "affiliation": "central drug research institute"}, {"name": "karuna shankar", "affiliation": "central institute of medicinal and aromatic plants"}, {"name": "ashok sharma", "affiliation": "central institute of medicinal and aromatic plants"}, {"name": "atul gupta", "affiliation": "central institute of medicinal and aromatic plants"}], "id": 2481244646}, {"title": "water sensitive urban design an investigation of current systems implementation drivers community perceptions and potential to supplement urban water services", "year": 2016, "source": "water", "authors": [{"name": "ashok sharma"}, {"name": "david pezzaniti"}, {"name": "baden myers"}, {"name": "stephen cook"}, {"name": "grace tjandraatmadja"}, {"name": "priya chacko"}, {"name": "sattar chavoshi"}, {"name": "david kemp"}, {"name": "rosemary leonard"}, {"name": "barbara a koth"}, {"name": "andrea walton"}], "id": 134216166}], "ANDS-Grants": [{"description": "The Australian water industry faces the challenge of catering for the potable water demand of a rapidly expanding population with reduced reliability on supply imposed by an increasingly variable climate. Diversified water supply schemes (DWSS) incorporating decentralised systems or reuse sources are touted as a means to handle the inherent weaknesses of centralised urban water supply schemes by potentially drawing 30-50 per cent less demand on their reserves. This research study will provide evidence to support the implementation of best practice DWSS based on an evidence based holistic assessment of their performance considering potable water savings, capital and operation costs, energy demand, as well as environmental and community impacts.", "title": "Smart metering founding a holistic evidence-based performance evaluation framework and demand forecasting model for diversified water supply schemes", "id": "551736", "institutions": ["Griffith University"], "key": "http://purl.org/au-research/grants/arc/LP100200033", "researchers": ["Stewart Dr Rodney A", "Prof Sherif A Mohamed", "Dr Simon A Fane", "Dr Ashok Sharma"], "status": "closed", "fundingAmount": 270000, "fundingScheme": "Linkage Projects", "startDate": 2010, "endDate": 2013, "dateTimeModified": "2015-08-13T17:18:55Z", "dateTimeCreated": "2015-04-29T07:03:20Z", "funder": ["Australian Research Council"], "managingInstitution": ["Griffith University"], "principalInvestigator": null, "purl": "http://purl.org/au-research/grants/arc/LP100200033", "identifiers": ["http://purl.org/au-research/grants/arc/LP100200033", "LP100200033"], "subjects": ["ENVIRONMENTAL SCIENCE AND MANAGEMENT", "ENVIRONMENTAL SCIENCES", "diversified water supply schemes", "end use measurement", "energy efficiency", "smart metering", "urban water management", "water conservation", "water efficiency"], "links": [{"rel": "self", "href": "http://researchdata.edu.au/api/v2.0/registry/activities/grants/551736"}]}]}